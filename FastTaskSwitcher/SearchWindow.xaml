<Window x:Class="FastTaskSwitcher.SearchWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SearchWindow" Height="350" Width="333">
    <Grid>
        <TextBox x:Name="searchBox" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="304" TabIndex="0" TextChanged="searchBox_TextChanged"/>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="246" Margin="10,38,0,0" VerticalAlignment="Top" Width="304"/>
        <Button x:Name="okButton" Content="OK" HorizontalAlignment="Left" Margin="159,289,0,0" VerticalAlignment="Top" Width="75" TabIndex="1"/>
        <Button x:Name="cancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="239,289,0,0" VerticalAlignment="Top" Width="75" TabIndex="2"/>

    </Grid>
</Window>
